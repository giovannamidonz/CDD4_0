class Automovel:
    __placa = None

    def __init__(self,placa):
        self.placa = placa

    def get__placa(self):
        return self.placa

    def dirigir(self,velocidade):
        if velocidade > 0 :
            print("O Carro está em movimento")
        else:
            print("O carro está em repouso")

carro = Automovel('XX-123')
carro.dirigir(80)
