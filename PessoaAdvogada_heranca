from pessoa import Pessoa

class PessoaAdvogada(Pessoa):
    def __init__(self,oab,nome,idade,cpf,telefone):
        self.__oab = oab
        super().__init__(nome,idade,cpf,telefone)
        
    def getOab(self):
        return self.__oab
        
    def setOab(self,oab):
        self.__oab = oab
        
